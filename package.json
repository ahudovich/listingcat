{
  "name": "listingcat",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.11.1+sha512.e519b9f7639869dc8d5c3c5dfef73b3f091094b0a006d7317353c72b124e80e1afd429732e28705ad6bfa1ee879c1fce46c128ccebd3192101f43dd67c667912",
  "engines": {
    "node": "22"
  },
  "author": {
    "name": "Andrei Hudovich",
    "email": "andrei.hudovich@gmail.com",
    "url": "https://hudovich.com"
  },
  "homepage": "https://listingcat.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/AndreiHudovich/listingcat"
  },
  "scripts": {
    "prepare": "husky",
    "clean": "git clean -xdf .next node_modules",
    "start": "doppler run -p lc-main -c dev -- next start",
    "dev": "doppler run -p lc-main -c dev -- next dev --turbopack",
    "build": "next build",
    "lint": "doppler run -p lc-main -c dev -- next lint",
    "typecheck": "tsc --noEmit",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "auth:generate": "doppler run -p lc-main -c dev -- pnpm dlx @better-auth/cli@latest generate --config ./auth.config.ts --output ./lib/db/schema/tables/auth.ts",
    "db:check": "doppler run -p lc-main -c dev -- drizzle-kit check",
    "db:generate": "doppler run -p lc-main -c dev -- drizzle-kit generate",
    "db:migrate": "doppler run -p lc-main -c dev -- drizzle-kit migrate",
    "db:migrate:prod": "doppler run -p lc-main -c prd -- drizzle-kit migrate",
    "db:drop": "doppler run -p lc-main -c dev -- drizzle-kit drop",
    "trigger:dev": "doppler run -p lc-main -c dev -- pnpm dlx trigger.dev@latest dev",
    "trigger:deploy": "doppler run -p lc-main -c dev -- pnpm dlx trigger.dev@latest deploy"
  },
  "dependencies": {
    "@hugeicons/core-free-icons": "^1.0.14",
    "@hugeicons/react": "^1.0.5",
    "@sentry/nextjs": "^9.27.0",
    "@t3-oss/env-nextjs": "^0.13.6",
    "@tanstack/react-table": "^8.21.3",
    "@trigger.dev/sdk": "^3.3.17",
    "better-auth": "^1.2.8",
    "canvas-confetti": "^1.9.3",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.44.1",
    "lodash-es": "^4.17.21",
    "next": "^15.3.3",
    "ofetch": "^1.4.1",
    "postgres": "^3.4.7",
    "posthog-js": "^1.249.2",
    "posthog-node": "^5.1.1",
    "radix-ui": "^1.4.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-use": "^17.6.0",
    "resend": "^4.5.1",
    "sonner": "^2.0.5",
    "tailwind-merge": "^3.3.0",
    "tailwind-variants": "^1.0.0",
    "zod": "^3.25.51"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.2",
    "@tailwindcss/postcss": "^4.1.8",
    "@tailwindcss/typography": "^0.5.16",
    "@trigger.dev/build": "^3.3.17",
    "@types/canvas-confetti": "^1.9.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9.30.0",
    "eslint-config-next": "^15.3.4",
    "eslint-config-prettier": "^10.1.5",
    "husky": "^9.1.7",
    "jiti": "^2.4.2",
    "lint-staged": "^16.1.2",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.8",
    "tw-animate-css": "^1.3.3",
    "typescript": "^5.8.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild"
    ]
  }
}
