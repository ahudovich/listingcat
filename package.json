{
  "name": "listingcat-next",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.4.1+sha512.c753b6c3ad7afa13af388fa6d808035a008e30ea9993f58c6663e2bc5ff21679aa834db094987129aa4d488b86df57f7b634981b2f827cdcacc698cc0cfb88af",
  "engines": {
    "node": "22"
  },
  "author": {
    "name": "Andrei Hudovich",
    "email": "andrei.hudovich@gmail.com",
    "url": "https://hudovich.com"
  },
  "homepage": "https://listingcat.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/AndreiHudovich/listingcat"
  },
  "scripts": {
    "clean": "git clean -xdf .next node_modules",
    "start": "doppler run -p lc-main -c dev -- next start",
    "dev": "doppler run -p lc-main -c dev -- next dev --turbopack",
    "build": "next build",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "db:check": "doppler run -p lc-main -c dev -- drizzle-kit check",
    "db:generate": "doppler run -p lc-main -c dev -- drizzle-kit generate",
    "db:migrate": "doppler run -p lc-main -c dev -- drizzle-kit migrate",
    "db:migrate:prod": "doppler run -p lc-main -c prd -- drizzle-kit migrate",
    "db:drop": "doppler run -p lc-main -c dev -- drizzle-kit drop",
    "auth:generate": "doppler run -p lc-main -c dev -- pnpm dlx @better-auth/cli@latest generate --config ./auth.config.ts --output ./lib/db/schema/tables/auth.ts"
  },
  "dependencies": {
    "@hugeicons/core-free-icons": "^1.0.14",
    "@hugeicons/react": "^1.0.5",
    "@radix-ui/react-slot": "^1.2.3",
    "@tanstack/react-table": "^8.21.3",
    "better-auth": "^1.2.8",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.1",
    "lodash-es": "^4.17.21",
    "next": "^15.3.3",
    "postgres": "^3.4.7",
    "posthog-js": "^1.249.2",
    "radix-ui": "^1.4.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "resend": "^4.5.1",
    "tailwind-merge": "^3.3.0",
    "tailwind-variants": "^1.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@ianvs/prettier-plugin-sort-imports": "^4.4.2",
    "@tailwindcss/postcss": "^4.1.8",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^22.15.29",
    "@types/react": "^19.1.6",
    "@types/react-dom": "^19.1.5",
    "drizzle-kit": "^0.31.1",
    "eslint": "^9.28.0",
    "eslint-config-next": "^15.3.3",
    "eslint-config-prettier": "^10.1.5",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.1.8",
    "tw-animate-css": "^1.3.3",
    "typescript": "^5.8.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "sharp",
      "esbuild"
    ]
  }
}
